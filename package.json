{
  "name": "g8road-crm",
  "version": "1.0.0",
  "description": "Web-based CRM for G8Road camps to manage their roster",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm install && CI=false npm run build && cd .. && mv client/build build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "railway:start": "node server/index.js"
  },
  "keywords": [
    "g8road",
    "crm",
    "camp-management",
    "roster"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@mui/x-date-pickers": "^8.11.3",
    "@zoom/meetingsdk": "^4.0.7",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.40.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "socket.io": "^4.7.2",
    "twilio": "^5.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
